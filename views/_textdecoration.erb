<div class="container">
  
  <!-- TITLE -->
  <div class="col-md-12"> 
    <h2>Text Decoration</h2>
  </div>

      <!-- Tab panes -->
    <div class="tab-content">

      <!-- HTML FOR ENTRY HERE -->
      <div class="style-container"> <!-- TRYING TO USE THIS DIV TO CONTAIN STYLES TO ONLY APPLY WITHIN -->
        <ul class="demo">
          <li class="overline">overline</li>
          <li class="line-through">line-through</li>
          <li class="underline">underline</li>
          <li class="none">default (none)</li>
      </div>
      <aside>
        <p>To get rid of the decoration on lists, you actually need to use list-style-type: none;</p>
        <ul>
          <li>no bullet points</li>
          <li>no numbers</li>
        </ul>
      </aside>
      <!-- CSS FOR ENTRY HERE -->
        <style>
          .overline {
            text-decoration: overline;
          }
          .line-through {
            text-decoration: line-through;
          }
          .underline {
            text-decoration: underline;
          }
          li {
            list-style-type: none;
          }
          .inherit {
            text-decoration: inherit;
          }
        </style>
    </div>
    <!-- CODE TAB -->
      <div class="col-md-6 code"> 
        <h4> CSS </h4>
